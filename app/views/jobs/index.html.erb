<h1>Jobs</h1>
<p>Find me in app/views/jobs/index.html.erb</p>
<% @jobs.each do |job| %>
    <div class="card-job">
        <div class="card-job-infos">
            <h1><%= job.title %></h1>
            <h3><%= job.location %></h3>
            <p><%= job.description%></p>
        </div>
    </div>
<% end %>

    
    