<div class="container-job">
    <h1>Jobs</h1>
    <% @jobs.each do |job| %>
        <div class="card-job">
            <div class="card-job-infos">
                <h2><%= job.title %></h2>
                <p>Location: <%= job.location %></p>
                <p><%= job.description%></p>
            </div>
        </div>
    <% end %>
</div>

    
    